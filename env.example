# INET Dashboard Environment Variables
# Copy this file to .env and customize the values

# Flask Secret Key
# Generate a secure random key for production: python -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=dev-secret-key-change-in-production

# Flask Environment (development or production)
FLASK_ENV=development

# Server Host
# Use 0.0.0.0 to allow external connections, or 127.0.0.1 for localhost only
FLASK_HOST=0.0.0.0

# Server Port
FLASK_PORT=5000

